.box {
  width: 100%;
  height: 100%;
}
/* 微信授权 */
.warrant {
  margin-top: 30rpx;
  width: 300rpx;
  background: rgb(7,193,96);
  color: white;
}

.info {
  width: 100%;
  height: 150rpx;
  display: flex;
  background-image: url('/img/my/bg.png');
  background: white;
  padding: 0rpx 30rpx 30rpx 30rpx;
}

.info > image {
  width: 130rpx;
  border-radius: 130rpx;
  border: 8rpx solid orange;
}

.info > .name {
  margin: 30rpx 0rpx 0rpx 15rpx;
  font-size: 30rpx;
  color: black;
}
/* 我的订单 */
.order {
  width: 100%;
  height: 80rpx;
  font-size: 32rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #ddd;
}

.order > text{
  margin-left: 35rpx;
  float: left;
}
.order > navigator {
  margin-right: 35rpx;
  float: right;
}
.order > navigator > image  {
  transform: translateY(7rpx);
  width: 40rpx;
}
/* 订单详情 */
.orderSituation {
  text-align: center;
  display: flex;
  height: 180rpx;
  font-size: 25rpx;
}
.orderSituation > navigator {
  padding-top: 38rpx;
  width: 33.33%;
}
.orderSituation image {
  position: relative;
  width: 60rpx;
}

/* 分隔符 */
.ui-line {
  height: 20rpx;
  background: #f5f5f5;
}

.cells .cell image{
  margin-right: 16px;vertical-align: middle;width:20px; height: 20px;
}